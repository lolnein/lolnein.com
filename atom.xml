---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>lolnein</title>
	<id>urn:uuid:27848db4-8dd3-4bc8-8e74-196fef152250</id>
	<updated>{{ site.time | date_to_xmlschema }}</updated>

	<author>
		<name>Vincent Kunert</name>
		<email>vin@lolnein.com</email>
	</author>

	<generator uri="http://jekyllrb.com/">
		Jekyll
	</generator>

	<icon>/lolnein.png</icon>

	<link rel="self" href="http://lolnein.com/atom.xml"/>
	<link rel="alternate" href="http://lolnein.com/"/>

	<link rel="license" type="application/rdf+xml" href="http://creativecommons.org/licenses/by-nc-sa/3.0/rdf"/>
	<link rel="license" type="text/html" href="http://creativecommons.org/licenses/by-nc-sa/3.0/"/>
{% for post in site.posts limit:256 %}
	<entry xml:lang="en-GB-oed">
		<title>{{ post.title | xml_escape }}</title>
		<id>urn:x-lolnein:{{ post.id | xml_escape }}</id>
		<link rel="alternate" href="http://lolnein.com{{ post.url }}/"/>
		<published>{{ post.date | date: "%Y-%m-%d" }}</published>
		<updated>{% if post.update %}{{ post.update | xml_escape }}{% else %}{{ post.date | date: "%Y-%m-%d" }}{% endif %}</updated>

		<content type="xhtml">
			<article xmlns="http://www.w3.org/1999/xhtml">
				{% if page.layout == 'youtube' %}<iframe width="560" height="315" src="{{ post.video }}" allowfullscreen="allowfullscreen" seamless="seamless"></iframe>{% else %}<img src="http://lolnein.com/comics/{{ post.comic }}"/>{% endif %}
				{{ post.content }}
			</article>
		</content>
{% for keyword in post.keywords limit:16 %}
		<category term="{{ keyword | xml_escape }}"/>{% endfor %}
	</entry>{% endfor %}
</feed>
